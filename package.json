{
  "name": "geoio",
  "version": "0.0.1",
  "description": "A small Geo IO library.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianfrey/geoio.git"
  },
  "keywords": [
    "js",
    "geo",
    "io",
    "shape"
  ],
  "author": "sfrey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebastianfrey/geoio/issues"
  },
  "homepage": "http://geoapplications.de/geoio/",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "bluebird": "^3.5.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "leaflet": "^1.2.0",
    "leaflet-draw": "^0.4.12",
    "leaflet.markercluster": "^1.1.0",
    "lodash": "^4.17.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "proj4": "^2.4.4",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-app-rewired": "^1.3.3",
    "react-color": "^2.13.8",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.1",
    "react-icons-kit": "^1.0.7",
    "react-leaflet": "^1.7.1",
    "react-leaflet-draw": "^0.16.0",
    "react-notification-system": "^0.2.16",
    "react-notification-system-redux": "^1.2.0",
    "react-redux": "^5.0.6",
    "react-splitter-layout": "^3.0.0",
    "redux": "^3.7.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3",
    "worker-loader": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
